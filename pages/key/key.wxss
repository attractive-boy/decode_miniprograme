/* pages/key/key.wxss */
.button-example {
  margin: 32rpx;
  position: absolute;
  bottom: 10%;
  width: calc(100% - 64rpx);
}
.btn-wrapper {
  height: 100%;
}

.btn {
  display: inline-flex;
  justify-content: center;
  align-items: center;
  width: 120rpx;
  height: 100%;
  color: white;
}

.delete-btn {
  background-color: #e34d59;
}

.edit-btn {
  background-color: #ed7b2f;
}

.favor-btn {
  background-color: var(--td-brand-color, #0052d9);
}
